{
    "name": "deviouspk/supremewatcher-backend",
    "description": "supremewatcher-backend",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-hash": "*",
        "ext-json": "*",
        "fideloper/proxy": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "hhxsv5/laravel-s": "~3.0",
        "larapie/guard": "^1.1",
        "larapie/repository": "^3.0",
        "laravel/framework": "5.8.*",
        "nwidart/laravel-modules": "^4.0",
        "pusher/pusher-php-server": "^3.2",
        "larapie/data-transfer-object": "^2.0",
        "spatie/laravel-permission": "^2.37",
        "spatie/once": "^2.0",
        "stechstudio/laravel-php-cs-fixer": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Modules\\": "src/Modules/",
            "Foundation\\": "src/Foundation/"
        },
        "files": [
            "src/Foundation/Support/helpers.php",
            "src/Foundation/Support/cloudflare_helper.php"
        ]
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "src/Modules/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "laravel": {
            "dont-discover": [
                "nwidart/laravel-modules",
                "barryvdh/laravel-ide-helper",
                "laravel/horizon",
                "laravel/telescope",
                "jenssegers/mongodb",
                "spatie/laravel-permission",
                "auth0/login",
                "barryvdh/laravel-cors"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan bootstrap:cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
